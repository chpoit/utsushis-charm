{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug UI",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                // "--reset",
            ]
        },
        {
            "name": "Frame Extraction",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-charms",
                "--console",
                "--autoexit"
            ]
        },
        {
            "name": "Charm Extraction",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-frames",
                "--console",
                "--autoexit"
            ]
        },
        {
            "name": "Debug UI (With parse errors)",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-frames",
                "-f",
                "frames2"
            ]
        },
        {
            "name": "Charm Extraction (With parse errors, Console)",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-frames",
                "--console",
                "-f",
                "frames2",
                "--autoexit"
            ]
        },
        {
            "name": "Charm Extraction (French)",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-frames",
                "--console",
                "-l",
                "French",
                "--autoexit"
            ]
        },
        {
            "name": "Debug Charm Extraction",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--skip-frames",
                "--frames",
                "debug-frames",
                "--console",
                "--autoexit"
            ]
        }
    ]
}