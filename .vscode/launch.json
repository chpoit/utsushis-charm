    "version": "0.2.0",
    "configurations": [
    {
        "name": "Node encoder",
        "program": "${workspaceFolder}/js_encoder.js",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "pwa-node",
        "args": ["charms.single.json"]
    },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "decompress",
            "type": "python",
            "request": "launch",
            "program":"mp4_decompress.py",
            "console": "integratedTerminal"
        },
        {
            "name": "get_charms",
            "type": "python",
            "request": "launch",
            "program":"extract_charms.py",
            "console": "integratedTerminal"
        }
    ]
}